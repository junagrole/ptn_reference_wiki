filters:
  and:
    - State == "Complete"
    - '!file.inFolder("Templates")'
views:
  - type: table
    name: Tags
    groupBy:
      property: Content
      direction: ASC
    order:
      - file.name
      - tags
  - type: table
    name: Meta Info
    groupBy:
      property: Content
      direction: ASC
    order:
      - file.name
      - file.folder
      - file.ctime
      - file.mtime
      - aliases
